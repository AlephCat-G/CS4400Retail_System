<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="your.package.MainDashboardController"
      xmlns:fx="http://javafx.com/fxml/1" spacing="10">
    <Button text="Customers" fx:id="customersButton" onAction="#handleCustomers"/>
    <Button text="Products" fx:id="productsButton" onAction="#handleProducts"/>
    <Button text="Drones" fx:id="dronesButton" onAction="#handleDrones"/>
    <Button text="Pilots" fx:id="pilotsButton" onAction="#handlePilots"/>
    <Button text="Orders" fx:id="ordersButton" onAction="#handleOrders"/>
    <Button text="Views" fx:id="viewsButton" onAction="#handleViews"/>
</VBox>
